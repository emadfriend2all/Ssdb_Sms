@using MudBlazor
<div class="pb-6">
    <PageTitle>@Title</PageTitle>
    <MudText Typo="Typo.h6">@Header</MudText>
    <MudText Typo="Typo.caption" >@SubHeader</MudText>
</div >
@code {
    [Parameter]
    public required string Title { get; set; }

    [Parameter]
    public required string Header { get; set; }

    [Parameter]
    public required string SubHeader { get; set; }
}
